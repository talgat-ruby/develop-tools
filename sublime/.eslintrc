{
	"extends": ["plugin:react/recommended", "react-app", "eslint:recommended", "plugin:flowtype/recommended"],
	"plugins": [
    "react",
    "flowtype"
  ],
	"rules": {
		"no-await-in-loop": 1,
		"class-methods-use-this": 1,
		"default-case": 2,
		"dot-notation": 2,
		"eqeqeq": 2,
		"no-alert": 1,
		"no-caller": 2,
		"no-empty-function": 2,
		"no-eq-null": 2,
		"no-eval": 2,
		"no-floating-decimal": 1,
		"no-implicit-coercion": 2,
		"no-invalid-this": 1,
		"no-iterator": 1,
		"no-magic-numbers": [2, {"ignore": [-1, 0, 1]}],
		"no-multi-spaces": 1,
		"no-param-reassign": 2,
		"no-proto": 1,
		"no-return-assign": 1,
		"no-self-compare": 2,
		"no-undefined": 2,
		"arrow-body-style": [2, "as-needed"],
		"no-duplicate-imports": 2,
		"no-var": 2,
		"object-shorthand": 2,
		"prefer-const": 2,
		"prefer-rest-params": 2,
		"prefer-spread": 2,
		"prefer-template": 2,
		"symbol-description": 2,
		"template-curly-spacing": 2,
		"react/no-unused-state": 2,
		"react/prefer-stateless-function": 1,
		"react/self-closing-comp": 1,
		"react/jsx-boolean-value": 1,
		"react/jsx-key": 2,
		"react/jsx-no-bind": 2
	}
}
